  "description": "Single spa project - Grupo 03 - ADS UNMSM",
  "main": "index.js",
  "scripts": {
    "start": "run-p start:dev",
    "start:dev": "webpack-dev-server --config webpack.config.dev.js",
    "clean:build": "rimraf ./build && mkdir build",
    "copy:manifest": "node -e \"require('fs').copyFile('./manifest.yml', './build/manifest.yml', function(err) { if (err) console.log(err); console.log('manifest.yml copied!') })\"",
    "prebuild": "run-p clean:build",
    "build": "webpack --config webpack.config.prod.js",
    "postbuild": "run-p copy:manifest",
    "serve:build": "http-server ./build"
  },
